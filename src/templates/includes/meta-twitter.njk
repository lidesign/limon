<!-- Twitter card -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="{{ config.twitter.site }}">
<meta name="twitter:creator" content="{{ config.twitter.creator }}">
<meta name="twitter:url" content="{{ config.url }}">
<meta name="twitter:title" content="{{ config.shortTitle }}">
<meta name="twitter:description" content="{{ config.description }}">
<meta name="twitter:image" content="{{ config.url + config.images }}">
